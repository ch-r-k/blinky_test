{
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "C_Cpp.codeAnalysis.runAutomatically": true,
    "C_Cpp.codeAnalysis.exclude": {
        "**/3d_party/**": false
    },
    "clang-format.style": "file",
    "editor.formatOnSave": true,
    "files.associations": {
        "cstdarg": "cpp",
        "cstdint": "cpp",
        "new": "cpp",
        "type_traits": "cpp"
    },
    "cmake.configureOnOpen": true
}