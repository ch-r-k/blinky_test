    "version": "0.2.0",
    "configurations": [
      {
        "type": "cortex-debug",
        "request": "launch",
        "name": "J-Link",
        "cwd": "${workspaceRoot}",
        "armToolchainPath": "/opt/gcc-arm-none-eabi/bin/",
        "executable": "${workspaceRoot}/build/blinky",
        "serverpath": "JLinkGDBServerCLExe",
        "ipAddress": "172.16.254.106",
        "runToEntryPoint": "main",
        "servertype": "jlink",
        "showDevDebugOutput": "both",
        "device": "STM32L476RG",
        "interface": "swd",
        //"svdFile": "${workspaceRoot}/sdk/ONSemiconductor/svd/rsl10.svd",
        "rttConfig": {
          "enabled": true,
          "address": "auto",
          "decoders": [
            {
              "label": "",
              "port": 0,
              "type": "console"
            }
          ]
        }
      }
    ]
  }
